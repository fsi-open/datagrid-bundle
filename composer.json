{
    "name": "fsi/datagrid-bundle",
    "type": "symfony-bundle",
    "description": "FSi Datagrid Bundle",
    "keywords": ["fsi", "symfony", "datagrid", "datagrid-bundle", "bundle"],
    "license": "MIT",
    "authors": [
        {
            "name": "Norbert Orzechowicz",
            "email": "norbert@fsi.pl"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "twig/twig": ">=1.2,<2.0-dev",
        "symfony/framework-bundle" : "2.1.*",
        "symfony/http-foundation": "2.1.*",
        "fsi/datagrid": ">=0.9.0,<=1.0.*-dev"
    },
    "autoload": {
        "psr-0": { "FSi\\Bundle\\DataGridBundle\\": "" }
    },
    "suggest": {
        "phpoffice/phpexcel" : "Allows export data to xls format.",
        "fsi/fsi-extension-datagrid": "Allows more advanced usage of datagrid. You should search this in fsi private repositories."
    },
    "target-dir" : "FSi/Bundle/DataGridBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
