{
    "name": "fsi/datagrid-bundle",
    "type": "symfony-bundle",
    "description": "FSi Datagrid Bundle",
    "keywords": ["fsi", "symfony", "datagrid", "datagrid-bundle", "bundle"],
    "license": "MIT",
    "authors": [
        {
            "name": "Norbert Orzechowicz",
            "email": "norbert@orzechowicz.pl"
        }
    ],
    "require": {
        "php": "^7.1|^8.0",
        "twig/twig": "^2.11",
        "symfony/property-access": "^3.4|^4.0|^5.0",
        "symfony/framework-bundle" : "^3.4|^4.0|^5.0",
        "symfony/options-resolver": "^3.4|^4.0|^5.0",
        "fsi/datagrid": "^2.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.66",
        "phpunit/phpunit": "^7.5|^8.0",
        "squizlabs/php_codesniffer": "^3.4",
        "symfony/form" : "^3.4|^4.0|^5.0",
        "symfony/validator" : "^3.4|^4.0|^5.0",
        "symfony/translation" : "^3.4|^4.0|^5.0",
        "symfony/doctrine-bridge": "^3.4|^4.0|^5.0",
        "symfony/phpunit-bridge": "^4.3|^5.0",
        "symfony/security-csrf": "^3.4|^4.0|^5.0",
        "symfony/twig-bridge" : "^3.4|^4.0|^5.0",
        "symfony/yaml" : "^3.4|^4.0|^5.0"
    },
    "autoload": {
        "psr-4": { "FSi\\Bundle\\DataGridBundle\\": "" }
    },
    "suggest": {
        "phpoffice/phpexcel" : "Allows exporting data to xls format."
    },
    "config": {
        "bin-dir": "vendor/bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev",
            "2.1": "2.1-dev",
            "2.0": "2.0-dev",
            "1.2": "1.2-dev",
            "1.1": "1.1-dev",
            "1.0": "1.0-dev"
        }
    }
}
