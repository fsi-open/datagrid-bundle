{
    "name": "fsi/datagrid-bundle",
    "type": "symfony-bundle",
    "description": "FSi Datagrid Bundle",
    "keywords": ["fsi", "symfony", "datagrid", "datagrid-bundle", "bundle"],
    "license": "MIT",
    "authors": [
        {
            "name": "Norbert Orzechowicz",
            "email": "norbert@orzechowicz.pl"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "twig/twig": "~1.2",
        "symfony/framework-bundle" : "~2.2",
        "fsi/datagrid": "1.0.*"
    },
    "require-dev": {
        "symfony/form" : "2.2.*",
        "symfony/twig-bridge" : "2.2.*",
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-0": { "FSi\\Bundle\\DataGridBundle\\": "" }
    },
    "suggest": {
        "phpoffice/phpexcel" : "Allows export data to xls format."
    },
    "config": {
        "bin-dir": "bin"
    },
    "target-dir" : "FSi/Bundle/DataGridBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    }
}
